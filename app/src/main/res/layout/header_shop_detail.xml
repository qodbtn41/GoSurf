<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="위치보기"
        android:id="@+id/button6"
        android:layout_below="@+id/textView14"
        android:layout_toRightOf="@+id/button7"
        android:layout_toEndOf="@+id/button7" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="평점남기기"
        android:id="@+id/button7"
        android:layout_below="@+id/textView14"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="댓글쓰기"
        android:id="@+id/btn_comment"
        android:layout_below="@+id/textView14"
        android:layout_toLeftOf="@+id/button7"
        android:layout_toStartOf="@+id/button7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="죽도 Blue Coast"
        android:id="@+id/textView13"
        android:layout_below="@+id/ratingBar2"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="서핑장비대여, 서핑강습(스쿨), 서핑용품판매, 숙박, 게스트하우스, 펍, 레스토랑등 제공."
        android:id="@+id/textView14"
        android:layout_below="@+id/textView13"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:id="@+id/imageView9"
        android:layout_gravity="left|top"
        android:src="@android:drawable/sym_def_app_icon"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton11"
        android:layout_gravity="center_horizontal|top"
        android:src="@android:drawable/ic_menu_info_details"
        android:layout_alignBottom="@+id/imageView9"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/image"
        android:layout_gravity="center_horizontal|top"
        android:src="@android:drawable/ic_menu_call"
        android:layout_alignTop="@+id/imageButton11"
        android:layout_toLeftOf="@+id/imageButton11"
        android:layout_toStartOf="@+id/imageButton11" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton13"
        android:layout_gravity="center_horizontal|top"
        android:src="@android:drawable/ic_menu_search"
        android:layout_alignBottom="@+id/imageView9"
        android:layout_toRightOf="@+id/imageButton11"
        android:layout_toEndOf="@+id/imageButton11" />

    <RatingBar
        style="@android:style/Widget.DeviceDefault.RatingBar.Small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/ratingBar2"
        android:layout_below="@+id/imageView9"
        android:layout_centerHorizontal="true" />
</RelativeLayout>